<?xml version="1.0" encoding="utf-8"?>
<Containers>
  <TemplateURL>https://raw.githubusercontent.com/kieve/docker-templates/master/kieve/stream-badge.xml</TemplateURL>
  <Beta>False</Beta>
  <Category>Tools:</Category>
  <Name>stream-badge</Name>
  <Description>
    Small tool that creates a profile image for Twitch streams. Useful for forum signatures.
  </Description>
  <Overview>
    Small tool that creates a profile image for Twitch streams. Useful for forum signatures.
  </Overview>
  <Support></Support>
  <Registry>https://registry.hub.docker.com/u/kieve/stream-badge/</Registry>
  <GitHub>https://github.com/kieve/stream-badge</GitHub>
  <Repository>kieve/stream-badge</Repository>
  <Project></Project>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
    <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>8080</HostPort>
        <ContainerPort>8080</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Environment>
    <Variable>
      <Name>UMASK</Name>
      <Value>000</Value>
    </Variable>
    <Variable>
      <Name>PUID</Name>
      <Value>99</Value>
    </Variable>
    <Variable>
      <Name>PGID</Name>
      <Value>100</Value>
    </Variable>
  </Environment>
  <Data>
    <Volume>
      <HostDir/>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <ExtraParams></ExtraParams>
</Containers>
