<?xml version="1.0" encoding="utf-8"?>
<Containers>
    <Name>hdmi-controller</Name>
    <Registry>http://10.81.1.1:5000/</Registry>
    <Repository>hdmi-controller</Repository>
    <Shell>bash</Shell>
    <Privileged>false</Privileged>
    <BindTime>true</BindTime>
    <Networking>
        <Mode>bridge</Mode>
        <Publish>
            <Port>
                <HostPort>8081</HostPort>
                <ContainerPort>8080</ContainerPort>
                <Protocol>tcp</Protocol>
            </Port>
        </Publish>
    </Networking>
    <Environment>
        <Variable>
            <Name>SERIAL_PORT</Name>
            <Value>/dev/ttyS0</Value>
        </Variable>
        <Device>
            <Name>SERIAL_PORT</Name>
            <Value>/dev/ttyS0</Value>
        </Device>
    </Environment>
    <Data>
        <Volume>
            <HostDir>/mnt/cache/appdata/hdmi-controller</HostDir>
            <ContainerDir>/tmp</ContainerDir>
            <Mode>rw</Mode>
        </Volume>
    </Data>
</Containers>
