<?xml version="1.0" encoding="utf-8"?>
<Containers>
  <TemplateURL>https://raw.githubusercontent.com/kieve/docker-templates/master/kieve/hdmi-controller.xml</TemplateURL>
  <Beta>False</Beta>
  <Category>Tools:</Category>
  <Name>hdmi-controller</Name>
  <Description>
  Web Server that interfaces with a TESmart HDMI 4x4 matrix over a Serial Port
  </Description>
  <Overview>
  Web Server that interfaces with a TESmart HDMI 4x4 matrix over a Serial Port
  </Overview>
  <Support></Support>
  <Registry>http://10.81.1.1/</Registry>
  <Repository>hdmi-controller</Repository>
  <Project></Project>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
    <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>8081</HostPort>
        <ContainerPort>8080</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Environment>
    <Variable>
      <Name>SERIAL_PORT</Name>
      <Value>/dev/ttyS0</Value>
    </Variable>
    <Device>
      <Name>SERIAL_PORT</Name>
      <Value>/dev/ttyS0</value>
    </Device>
  </Environment>
  <Data>
    <Volume>
      <HostDir>/mnt/cache/appdata/hdmi-controller</HostDir>
      <ContainerDir>/tmp</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <ExtraParams></ExtraParams>
</Containers>
