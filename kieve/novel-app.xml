<?xml version="1.0" encoding="utf-8"?>
<Container>
    <Name>novel-app</Name>
    <Registry>http://localhost:5000/</Registry>
    <Repository>localhost:5000/novel-app</Repository>
    <Shell>bash</Shell>
    <Privileged>false</Privileged>
    <BindTime>true</BindTime>
    <Networking>
        <Mode>bridge</Mode>
        <Publish>
            <Port>
                <HostPort>8086</HostPort>
                <ContainerPort>8080</ContainerPort>
                <Protocol>tcp</Protocol>
            </Port>
        </Publish>
    </Networking>
    <Data>
        <Volume>
            <HostDir>/mnt/cache/appdata/novel-app</HostDir>
            <ContainerDir>/config</ContainerDir>
            <Mode>rw</Mode>
        </Volume>
    </Data>
</Container>
